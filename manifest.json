{
  "manifest_version": 3,
  "name": "Clonocto",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "js/clonocto.js",
        "js/notes.js"
      ],
      "css": [
        "css/content.css"
      ]
    }
  ],
  "action": {
    "default_popup": "html/popup.html"  
  }
}